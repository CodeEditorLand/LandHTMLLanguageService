{
	"name": "vscode-html-languageservice",
	"version": "0.0.1",
	"private": false,
	"description": "Language service for HTML",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandHTMLLanguageService#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandHTMLLanguageService/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandHTMLLanguageService.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./lib/umd/htmlLanguageService.js",
	"module": "./lib/esm/htmlLanguageService.js",
	"typings": "./lib/umd/htmlLanguageService",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"clean": "rimraf lib",
		"compile": "tsc -p ./src && npm run copy-jsbeautify",
		"compile-esm": "tsc -p ./src/tsconfig.esm.json",
		"copy-jsbeautify": "node ./build/copy-jsbeautify.js",
		"install-types-next": "npm i vscode-languageserver-types@next -f -S && npm i vscode-languageserver-textdocument@next -f -S",
		"prepack": "npm run clean && npm run compile-esm && npm run test && npm run remove-sourcemap-refs",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"remove-sourcemap-refs": "node ./build/remove-sourcemap-refs.js",
		"update-data": "npm i @vscode/web-custom-data -D && node ./build/generateData.js",
		"update-jsbeautify": "npm i js-beautify && node ./build/update-jsbeautify.js",
		"update-jsbeautify-next": "npm i js-beautify@next && node ./build/update-jsbeautify.js"
	},
	"dependencies": {},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.x",
		"js-beautify": "1.15.1",
		"rimraf": "5.0.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
